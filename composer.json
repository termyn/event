{
    "name": "termyn/mesh",
    "description": "IEA based on the Symfony Messenger 6.* and higher",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Edo Barnas",
            "email": "iam@edobarnas.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Termyn\\Mesh\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Termyn\\Mesh\\Test\\": "tests"
        }
    },
    "require": {
        "php": "^8.2",
        "symfony/messenger": "6.*",
        "symfony/validator": "6.*",
        "termyn/id": "dev-main",
        "termyn/date-time": "dev-main"
    },
    "require-dev": {
        "phpstan/phpstan": "1.10.*",
        "phpstan/phpstan-phpunit": "1.3.*",
        "phpunit/phpunit": "10.1.*",
        "symplify/easy-coding-standard": "11.3.*"
    }
}
